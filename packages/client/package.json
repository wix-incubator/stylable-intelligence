{
  "name": "stylable-intelligence",
  "version": "0.0.1",
  "description": "Stylable Language Services Client",
  "author": "Nadav Abraami (nadav@wix.com)",
  "publisher": "Wix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:wixplosives/stylable-intelligence.git"
  },
  "bugs": {
    "url": "https://github.com/wixplosives/stylable-intelligence/issues"
  },
  "engines": {
    "vscode": "^1.14.2"
  },
  "categories": [
    "Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css"
  ],
  "main": "dist/src/extension.js",
  "files": [
    "dist/src"
  ],
  "testGlob": "./dist/test/**/*.spec.js?(x)",
  "homepage": "https://github.com/wixplosives/stylable-intelligence#readme",
  "keywords": [
    "typescript",
    "tdd"
  ],
  "browser": "dist/src/index.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "build": "tsc -d",
    "compile": "tsc -watch -d",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "reset": "rimraf node_modules dist && yarn install && yarn test",
    "pretest": "rimraf dist && yarn build",
    "test": "node ./run-test",
    "test:node": "mocha --reporter mocha-env-reporter dist/test/*.test.js"
  },
  "devDependencies": {
    "@types/chai": "~4.0.1",
    "@types/mocha": "~2.2.41",
    "@types/node": "^8.0.13",
    "@types/sinon": "~2.3.1",
    "@types/sinon-chai": "~2.7.28",
    "chai": "~4.1.0",
    "chai-subset": "~1.5.0",
    "concurrently": "~3.5.0",
    "glob": "^7.1.2",
    "mocha": "~3.4.2",
    "mocha-env-reporter": "~2.0.4",
    "mocha-loader": "~1.1.1",
    "rimraf": "~2.6.1",
    "sinon": "~2.3.5",
    "sinon-chai": "~2.12.0",
    "source-map-support": "~0.4.15",
    "tslib": "~1.7.1",
    "typescript": "~2.4.1",
    "vscode": "1.1.4"
  },
  "peerDependencies": {
    "tslib": "~1.7.1"
  },
  "dependencies": {
    "@types/glob": "^5.0.30",
    "@types/lodash": "4.14.71",
    "css-selector-tokenizer": "^0.7.0",
    "lodash": "4.17.4",
    "postcss": "^6.0.7",
    "postcss-nested": "^2.0.4",
    "stylable": "^3.0.0",
    "vscode-languageclient": "^3.3.0",
    "vscode-languageserver": "^3.3.0"
  }
}

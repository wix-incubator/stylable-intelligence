{
  "compilerOptions": {
    "lib": [
      "es2018"
    ],
    "module": "commonjs",
    "target": "es2018",
    "strict": true,
    "importHelpers": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist"
  },
  "exclude": [
    "service.d.ts",
    "node_modules",
    "dist"
  ]
}

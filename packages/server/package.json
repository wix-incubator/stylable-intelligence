{
  "name": "stylable-intelligence-server",
  "version": "0.0.1",
  "description": "Stylable Language Server",
  "author": "Nadav Abraami (nadav@wix.com)",
  "publisher": "Nadav Abraami (nadav@wix.com)",
  "license": "MIT",
  "engines": {
    "node": "*",
    "vscode": "^1.14.2"
  },
  "testGlob": "../client/server/test/**/*.spec.js?(x)",
  "scripts": {
    "clear": "rimraf ../client/server",
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p .",
    "build": "tsc -d",
    "pretest": "yarn clear && yarn build",
    "test":  "mocha  --reporter mocha-env-reporter ../client/server/test/**/*.spec.js",
    "start": "yarn clear && yarn build && concurrently --kill-others --success first \"yarn build -- -w\" \"webpack-dev-server --no-info\""
  },
  "devDependencies": {
    "@types/chai": "~4.0.1",
    "@types/mocha": "~2.2.41",
    "@types/node": "^8.0.13",
    "@types/sinon": "~2.3.1",
    "@types/sinon-chai": "~2.7.28",
    "chai": "~4.1.0",
    "chai-subset": "^1.5.0",
    "concurrently": "~3.5.0",
    "glob": "~7.1.2",
    "mocha": "~3.4.2",
    "mocha-env-reporter": "~2.0.4",
    "mocha-loader": "~1.1.1",
    "rimraf": "~2.6.1",
    "sinon": "~2.3.5",
    "sinon-chai": "~2.12.0",
    "source-map-support": "~0.4.15",
    "tslib": "~1.7.1",
    "typescript": "~2.4.1",
    "webpack": "~3.3.0",
    "webpack-dev-server": "~2.6.1"
  },
  "peerDependencies": {
    "tslib": "~1.7.1"
  },
  "dependencies": {
    "@types/lodash": "4.14.71",
    "css-selector-tokenizer": "^0.7.0",
    "lodash": "4.17.4",
    "postcss": "^6.0.7",
    "postcss-nested": "^2.0.4",
    "stylable": "^3.0.0",
    "vscode-languageclient": "^3.3.0",
    "vscode-languageserver": "^3.3.0"
  }
}

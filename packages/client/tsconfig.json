{
  "compilerOptions": {
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "target": "es5",

    "strict": true,
    "importHelpers": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "server",
    "dist"
  ]
}
